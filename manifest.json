{
  "manifest_version": 3,
  "name": "PromKatoo: AI Prompt Manager",
  "version": "1.0.1",
  "description": "Free AI Prompt Manager for Chrome. Organize, search, and reuse prompts for ChatGPT, Claude, Gemini, and more.",
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_title": "AI Prompt Manager"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}